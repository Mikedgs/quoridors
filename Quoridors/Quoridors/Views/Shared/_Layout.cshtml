<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Corridor</title>
    @Styles.Render("~/Content/Board.css")
    @Styles.Render("~/Content/grid.css")
    @Styles.Render("~/Content/normalize.css")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</head>
<body>
    <div class="container">
        @RenderBody()
        <footer>
            <p>&copy; @DateTime.Now.Year - Corridor</p>
        </footer>
    </div>

   
</body>
</html>
